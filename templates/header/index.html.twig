                                                 alt="menu_banner1"
                                                src="/assets/images/collections/{{megacolle.imageUrl}}">
                                            <div  class="banne_info">
                                                <h6 >{{megacolle.description}}</h6>
                                                <h4 >{{megacolle.title}}</h4><a
                                                     href="{{megacolle.ButtonLink}}">{{megacolle.ButtonText}}</a>
                                            </div>
                                        </div>
                                    </div>
                                    {% endfor %}
                                   
