                                <ul >
                                {%for head in app.session.get("headerPages") %}
                                    <li ><a 
                                            class="dropdown-item nav-link nav_item" 
                                            link="/page/{{head.slug}}"
                                            href="/page/{{head.slug}}">{{head.title}}</a>
                                    </li>
                                {%endfor%}
                                   
